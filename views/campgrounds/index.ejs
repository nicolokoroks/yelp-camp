<% layout('layouts/boilerplate') %>
    <h1>All campgrounds</h1>
    <div>
        <a href="/campgrounds/new">Make a new campground</a>
    </div>
    <ul>
        <% for (let campground of campgrounds) {%>
            <li><a href="/campgrounds/<%= campground.id %>">
                    <%= campground.title %>
                </a></li>
            <% } %>
    </ul>